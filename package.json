{
  "name": "vuejs-starter",
  "version": "1.0.1",
  "description": "vuejs starter boilerplate with webpack 3",
  "main": "src/index.js",
  "scripts": {
    "patch": "npm version bump",
    "watch": "webpack-dev-server --config webpack.config.js",
    "build": "rm -rf ./dist && npm run build:client",
    "build:client": "NODE_ENV=production webpack --config webpack.config.js -p --progress",
    "webpack-defaults": "webpack-defaults"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mathieu-R/vuejs-starter"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "vue"
  ],
  "author": "Mathieu-R",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mathieu-R/vuejs-starter/issues"
  },
  "homepage": "https://github.com/Mathieu-R/vuejs-starter#readme",
  "dependencies": {
    "babel-preset-minify": "^0.2.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "vue": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1"
  }
}
